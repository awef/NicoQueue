{
  "name": "NicoQueue",
  "version": "0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "browser_action": {
    "default_icon": "icon_19x19.png"
  },
  "icons" : {
    "16" : "icon_16x16.png",
    "48" : "icon_48x48.png",
    "128" : "icon_128x128.png"
  },
  "background": {
    "scripts": ["script.js"]
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs"
   ]
}
